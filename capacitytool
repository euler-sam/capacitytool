<script> //start of functions and variables
        var covid_count = 0;  //covid variable initialized at 0
	var flu_count = 0;  //flu variable initialized at 0
	
        //the function for the choice of covid 
        function covidAdd() {  
                covid_count = covid_count + 1;
	}
        //the function for the choice of the flu
        function fluAdd() {
		flu_count = flu_count + 1;
        }
        //the function for the results
	function resultsFunction() {
		var resultText = document.getElementById("results");
		var covidPercent = (covid_count/(covid_count + flu_count))*100;  //percent probability it is COVID-19
	        var fluPercent = (flu_count/(covid_count + flu_count))*100; //perent probability it is the flu
		resultText.innerHTML = "Percent probability it is COVID-19 =" + covidPercent + "<br> Percent probability it is the flu =" + fluPercent; //inner HTML
	}
	// The following code lets you know when the file was last modified.
        // This will help you know if you are looking at the new version of the file or if there is a delay on GitHub.
        var docMod = document.lastModified;
                alert("This file last modified  " + docMod);
                console.log("This file last modified  " + docMod);
        // If you find the alert box annoying, comment out that line and timestamp will still show up in the debugging console



 </script>



